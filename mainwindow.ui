/* ðŸ”¹ Tab Bar (The Strip Holding Tabs) */
QTabBar::tab {
    background: white;
    border: 1px solid #230A50; /* Darker purple, thin border */
    padding: 10px 20px;
    border-radius: 6px 6px 0px 0px;
    font-size: 14px;
    font-weight: bold;
    color: #555;
    transition: all 0.3s ease-in-out;
    min-width: 180px;
}

/* ðŸ”¹ Hover Effect */
QTabBar::tab:hover {
    background: qlineargradient(spread:pad, x1:0, y1:0, x2:0, y2:1, 
        stop:0 #5A1E8B, /* Dark purple */
        stop:1 #230A50); /* Even darker purple */
    color: white;
    border-color: #230A50; /* Darker purple */
}

/* ðŸ”¹ Selected Tab */
QTabBar::tab:selected {
    background: rgba(35,10,152,200); /* Blue */
    color: white;
    border-color: #230A50; /* Darker purple */
}

/* ðŸ”¹ Unselected Tabs */
QTabBar::tab:!selected {
    margin-top: 2px;
}

/* ðŸ”¹ Close Button on Tabs (if enabled) */
QTabBar::close-button {
    image: url(:/icons/close.png);
    padding: 5px;
}

QTabBar::close-button:hover {
    image: url(:/icons/close_hover.png);
}

